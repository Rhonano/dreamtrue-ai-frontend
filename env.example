# DreamTrue AI Frontend - Environment Variables
# Copy this file to .env.local and update with your actual values

# Google Cloud Configuration
REACT_APP_PROJECT_ID=your-gcp-project-id
REACT_APP_REGION=us-central1

# Backend API Configuration
REACT_APP_BACKEND_API_URL=https://your-backend-api-url.com
REACT_APP_API_VERSION=v1

# Vertex AI Configuration
REACT_APP_VERTEX_AI_PROJECT_ID=your-vertex-ai-project-id
REACT_APP_VERTEX_AI_LOCATION=us-central1

# Cloud Storage Configuration
REACT_APP_CLOUD_STORAGE_BUCKET=your-cloud-storage-bucket
REACT_APP_CLOUD_STORAGE_REGION=us-central1

# Authentication Configuration
REACT_APP_AUTH_DOMAIN=your-auth-domain.com
REACT_APP_AUTH_CLIENT_ID=your-auth-client-id

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-firebase-app-id

# Feature Flags
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_BRAND_SELECTOR=true

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=your-google-analytics-id

# Development Configuration
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info
